<?xml version="1.0" encoding="UTF-8" ?>
<object_types>


    <type id="Дом" importance="MIESTA" file="damy"> <!-- customClass="org.alex73.osm.validators.objects.CustomCheckHouse" -->
        <filter>
            <tag name="building" value="house" />
        </filter>
        <!-- required geometryType="AREA" osmTypes="way" customMethod="checkSize" / -->
        <allow>
            <tag name="building:levels" />
            <tag name="height" />
            <tag name="addr:country" />
            <tag name="addr:postcode" />
            <tag name="addr:city" />
            <tag name="addr:street" />
            <tag name="addr:housenumber" />
        </allow>
    </type>

    <type id="Будынак" importance="MIESTA" file="budynki"> <!-- customClass="org.alex73.osm.validators.objects.CustomCheckHouse" -->
